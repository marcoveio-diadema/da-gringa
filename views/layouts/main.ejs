            <div class="modal-body">
              <!-- search form -->
              <div class="search-nav-modal" id="searchNav">
                <form action="/blog/search" method="GET" class="px-4 py-3">
                  <div class="input-group">
                    <input type="text" name="q" class="form-control" placeholder="Busca rÃ¡pida">
                    <button class="btn btn-light search-button" type="submit">
                      <i class="fas fa-search"></i>
                    </button>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>

        <!-- content for the current page -->
        <main class="main">
            <%- body  %>
        </main>    

        <!-- footer partial -->
        <%- include('../partials/footer.ejs') %>

